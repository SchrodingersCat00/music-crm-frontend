<div class="row">
  <form class="col s12">
    <div class="row">
      <div class="col s12 m6 l2">
	      {{t-select options=prefixes value=sympathizer.prefix blankLabel="-" label="Aanspreking"}}
      </div>
      <div class="input-field col s12 m6 l2">
	      {{md-input value=sympathizer.organizationName label="Bedrijfsnaam"}}
      </div>
      <div class="input-field col s12 m6 l2">
	      {{md-input value=sympathizer.firstName label="Voornaam"}}
      </div>
      <div class="input-field col s12 m6 l2">
	      {{md-input value=sympathizer.lastName label="Achternaam"}}
      </div>
    </div>
    <div class="row">
      <div class="col s12 m6 l3">
        <div class="row">
          <h5>Adres</h5>
          <div class="input-field col s8 m8">
	          {{md-input value=sympathizer.address.street label="Straat"}}
          </div>
          <div class="input-field col s4 m4">
	          {{md-input value=sympathizer.address.number label="Nummer"}}
          </div>
          <div class="input-field col s4 m2">
	          {{md-input value=sympathizer.address.postCode label="Postcode"}}
          </div>
          <div class="input-field col s8 m10">
	          {{md-input value=sympathizer.address.city label="Gemeente"}}
          </div>
        </div>
      </div>
      <div class="col s12 m6 l3">
        <h5>Contactgegevens</h5>
        <div class="row">
          <div class="input-field col s12">
	          {{md-input value=sympathizer.email label="Email"}}
          </div>
          {{#each sympathizer.telephones as |telephone|}}
	          <div class="input-field col s12">
	            {{md-input value=telephone.value label="Telefoon"}}
	          </div>
          {{/each}}
        </div>
      </div>
    </div>
  </form>
</div>
