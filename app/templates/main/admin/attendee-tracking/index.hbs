<h3>Aanwezigheden</h3>

<div class="row">
  <div class="col s12">
    {{#link-to 'main.admin.attendeeTracking.statistics' class='btn waves-effect waves-light'}}<i class="material-icons left">insert_chart</i>Statistieken{{/link-to}}
  </div>
</div>
{{#md-collection content=model as |event|}}
  <div class='collection-item'>
    {{#link-to 'main.admin.attendeeTracking.track' event}}
      {{event.name}}
      <span class="secondary-content"><i class="material-icons">playlist_add_check</i></span>
    {{/link-to}}
  </div>
{{/md-collection}}

{{#link-to 'main.admin.attendeeTracking.add' class='btn-floating btn-large waves-effect waves-light right'}}<i class="material-icons">add</i>{{/link-to}}

