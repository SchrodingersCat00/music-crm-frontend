{{md-tabs content=tabs selected=activeTab colWidth=6}}

<div class="row">
  <div class="col s12"> 
    {{#if isActiveScores}}
      {{scores-table scores=activeScores archived=false addFile='addFile' savePart='savePart' delete='deleteScores' deletePart='deletePart' info='openScoreDetailModal' attach='openNewPartModal'}}
    {{else}}
      {{scores-table scores=archivedScores archived=true addFile='addFile' savePart='savePart' delete='deleteScores' deletePart='deletePart' info='openScoreDetailModal' attach='openNewPartModal'}}
    {{/if}}

    {{page-pagination pagination=model.meta.pagination changePage='changePage'}}
  </div>
</div>
{{score-detail-modal isOpen=detailModalIsOpen score=detailedScore deletePart='deletePart'}}
{{part-create-modal isOpen=createPartModalIsOpen score=scoreToCreatePartFor part=newPart addFile='addFile' savePart='savePart'}}
